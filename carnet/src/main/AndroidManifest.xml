<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.stoicavlad.carnet">
    <uses-sdk android:minSdkVersion="14"/>
    <application
        android:name="com.stoicavlad.carnet.CarnetApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.stoicavlad.carnet.ui.main.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.note.detail.NoteDetailActivity"
            android:parentActivityName="com.stoicavlad.carnet.ui.main.MainActivity"></activity>
    </application>

</manifest>
